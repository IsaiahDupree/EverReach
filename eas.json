{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "NPM_CONFIG_PACKAGE_MANAGER": "npm",
        "BUN_INSTALL": "0"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "env": {
        "NPM_CONFIG_PACKAGE_MANAGER": "npm",
        "BUN_INSTALL": "0"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "NPM_CONFIG_PACKAGE_MANAGER": "npm",
        "BUN_INSTALL": "0"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "isaiahdupree33@gmail.com",
        "ascAppId": "6738666193",
        "appleTeamId": "4Y7UYQP3HJ"
      }
    }
  }
}
