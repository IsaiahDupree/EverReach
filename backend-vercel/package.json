{
  "name": "personal-crm-backend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18 <21"
  },
  "scripts": {
    "dev": "next dev -p ${PORT:-3333}",
    "dev:3333": "next dev -p 3333",
    "build": "next build",
    "vercel-build": "npm install --no-save --os=linux --cpu=x64 sharp && next build",
    "start": "next start -p ${PORT:-3333}",
    "typecheck": "tsc --noEmit",
    "generate:openapi:ts": "openapi-typescript ./openapi/openapi.json -o ./openapi/types.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unified": "jest --config jest.config.unified.js",
    "test:unified:coverage": "jest --config jest.config.unified.js --coverage",
    "test:unified:watch": "jest --config jest.config.unified.js --watch",
    "test:unified:verbose": "jest --config jest.config.unified.js --verbose",
    "test:unified:ci": "jest --config jest.config.unified.js --coverage --ci --maxWorkers=2",
    "test:e2e:smoke": "node tests/e2e/api-smoke.mjs",
    "test:backend": "node tests/backend-comprehensive.test.mjs",
    "test:backend:local": "TEST_LOCAL=true TEST_PORT=3333 node tests/backend-comprehensive.test.mjs",
    "test:backend:prod": "TEST_BASE_URL=https://ever-reach-be.vercel.app node tests/backend-comprehensive.test.mjs",
    "test:subscription-source": "node tests/subscription-source.test.mjs",
    "test:master": "node tests/run-all.mjs",
    "test:master:verbose": "node tests/run-all.mjs --verbose",
    "test:master:skip-e2e": "node tests/run-all.mjs --skip-e2e",
    "test:warmth": "jest warmth-score",
    "test:warmth:modes": "node test/backend/warmth-modes.mjs",
    "test:warmth:modes:api": "node test/backend/warmth-modes-api.mjs",
    "test:warmth:modes:all": "node test/backend/run-warmth-modes-tests.mjs",
    "test:message": "jest message-send",
    "test:webhook": "jest posthog-webhook",
    "test:perf:jest": "jest message-generation-performance",
    "test:context": "jest context-assembly",
    "test:scenarios": "jest message-scenarios",
    "test:context-all": "jest context-assembly message-scenarios",
    "test:custom-fields": "jest custom-fields",
    "test:ad-pixels": "jest ad-pixels",
    "test:public-api": "jest public-api",
    "test:public-api-auth": "jest public-api-auth",
    "test:public-api-rate-limit": "jest public-api-rate-limit",
    "test:public-api-context": "jest public-api-context-bundle",
    "test:public-api-webhooks": "jest public-api-webhooks",
    "test:goal-inference": "jest goal-inference",
    "test:e2e:public-api": "jest __tests__/e2e",
    "test:e2e:public-api:auth": "jest __tests__/e2e/public-api-auth.e2e.test.ts",
    "test:e2e:public-api:context": "jest __tests__/e2e/public-api-context-bundle.e2e.test.ts",
    "test:e2e:deployed": "TEST_BASE_URL=https://ever-reach-be.vercel.app jest __tests__/e2e",
    "test:deployed:voice-notes": "node tests/run-voice-note-test.mjs",
    "test:deployed": "node tests/run-voice-note-test.mjs",
    "test:new-features": "jest warmth-score message-send posthog-webhook goal-inference",
    "test:all": "jest warmth-score message-send posthog-webhook message-generation-performance context-assembly message-scenarios custom-fields ad-pixels public-api goal-inference",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:watch": "vitest --config vitest.integration.config.ts",
    "test:integration:ui": "vitest --config vitest.integration.config.ts --ui",
    "test:integration:lifecycle": "vitest run __tests__/integration/contact-lifecycle.test.ts",
    "test:e2e": "vitest run __tests__/e2e/**/*.test.ts",
    "test:security": "vitest run __tests__/security/**/*.test.ts",
    "test:perf:vitest": "vitest run __tests__/performance/**/*.test.ts",
    "test:contract": "vitest run __tests__/contract/**/*.test.ts",
    "test:migrations": "node test/migrations/run-all.mjs",
    "test:migrations:occurred-at": "node test/migrations/test-occurred-at-migration.mjs",
    "test:marketing": "jest __tests__/marketing",
    "test:marketing:enrichment": "jest __tests__/marketing/enrichment.test.ts",
    "test:marketing:analytics": "jest __tests__/marketing/analytics.test.ts",
    "test:marketing:calculators": "jest __tests__/marketing/calculators.test.ts",
    "test:marketing:admin": "jest __tests__/marketing/admin-endpoints.test.ts",
    "test:marketing:watch": "jest __tests__/marketing --watch",
    "test:marketing:coverage": "jest __tests__/marketing --coverage",
    "test:integration:marketing": "jest __tests__/integration",
    "test:integration:marketing:flow": "jest __tests__/integration/marketing-complete-flow.integration.test.ts",
    "test:integration:marketing:third-party": "jest __tests__/integration/third-party-services.integration.test.ts",
    "test:integration:marketing:webhooks": "jest __tests__/integration/webhook-delivery.integration.test.ts",
    "test:integration:marketing:performance": "jest __tests__/integration/performance-load.integration.test.ts",
    "test:integration:marketing:all": "jest __tests__/integration --runInBand",
    "test:all-comprehensive": "npm run test:all && npm run test:integration && npm run test:e2e && npm run test:security && npm run test:migrations && npm run test:marketing && npm run test:integration:marketing",
    "test:services": "node test/integration/run-all-integration-tests.mjs",
    "test:services:revenuecat": "node test/integration/revenuecat.test.mjs",
    "test:services:stripe": "node test/integration/stripe.test.mjs",
    "test:services:supabase": "node test/integration/supabase.test.mjs",
    "test:services:openai": "node test/integration/openai.test.mjs",
    "test:services:posthog": "node test/integration/posthog.test.mjs",
    "test:services:superwall": "node test/integration/superwall.test.mjs",
    "test:services:resend": "node test/integration/resend.test.mjs",
    "test:services:twilio": "node test/integration/twilio.test.mjs",
    "test:services:meta": "node test/integration/meta.test.mjs",
    "test:voice-context": "npx tsx scripts/test-voice-context.ts",
    "test:voice-context:bash": "./scripts/test-voice-context.sh",
    "test:usage-limits": "node test/lib/usage-limits.test.mjs",
    "test:usage-limits:live": "node test/usage-limits-live.test.mjs"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.2",
    "@tanstack/react-query": "^5.90.6",
    "@trpc/server": "^11.6.0",
    "bcryptjs": "^2.4.3",
    "date-fns": "^4.1.0",
    "googleapis": "^137.0.0",
    "jose": "^5.9.6",
    "lucide-react": "^0.552.0",
    "next": "14.2.10",
    "openai": "^5.19.1",
    "p-queue": "^8.0.1",
    "posthog-node": "^4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-grid-layout": "^1.5.2",
    "recharts": "^3.3.0",
    "redis": "^4.7.0",
    "resend": "^3.2.0",
    "server-only": "^0.0.1",
    "sharp": "^0.34.5",
    "stripe": "^14.25.0",
    "superjson": "^2.2.2",
    "twilio": "^5.3.5",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-grid-layout": "^1.3.5",
    "@types/redis": "^4.0.11",
    "@types/uuid": "^9.0.8",
    "@vitest/ui": "^1.0.4",
    "jest": "^29.7.0",
    "jest-junit": "^15.0.0",
    "openapi-typescript": "^6.7.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.6.3",
    "vitest": "^1.0.4"
  }
}
