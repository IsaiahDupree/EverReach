# Backend base (Vercel production URL or your alias)
EXPO_PUBLIC_API_URL=https://ever-reach-be.vercel.app

# App mode
EXPO_PUBLIC_LOCAL_ONLY=false

# Development & Debug Settings
# Control visibility of developer-only features and debug information
# Set to 'false' or remove for production builds

# Master switch for all dev features (defaults to __DEV__ if not set)
EXPO_PUBLIC_ENABLE_DEV_FEATURES=true

# Show debug information sections (subscription info, state dumps, API logs)
EXPO_PUBLIC_SHOW_DEBUG_INFO=true

# Show developer settings and controls (test warmth modes, overrides, etc.)
EXPO_PUBLIC_SHOW_DEV_SETTINGS=true

# Enable detailed console logging (API calls, state changes, analytics)
EXPO_PUBLIC_ENABLE_DEBUG_LOGGING=true

# Show refresh/reload buttons (useful for beta testing)
EXPO_PUBLIC_SHOW_REFRESH_BUTTONS=true

# Enable experimental/beta features
EXPO_PUBLIC_ENABLE_EXPERIMENTAL=false

# Analytics (PostHog)
# Obtain a Project API key from https://app.posthog.com/project/settings
EXPO_PUBLIC_POSTHOG_API_KEY=
# Default US cloud host; change if you use EU or self-hosted PostHog
EXPO_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com

# Optional: Backend tracking base (if you add coverage APIs)
# Example: https://ever-reach-be.vercel.app
EXPO_PUBLIC_TRACKING_BASE_URL=

# Meta App Events / Conversions API
# Pixel ID from Meta Events Manager (https://business.facebook.com/events_manager)
EXPO_PUBLIC_META_PIXEL_ID=1191876055285693
# Conversions API access token — generate at Events Manager → Settings → Generate Access Token
EXPO_PUBLIC_META_CONVERSIONS_API_TOKEN=
# (Optional) Facebook App ID — required only for native SDK (react-native-fbsdk-next)
